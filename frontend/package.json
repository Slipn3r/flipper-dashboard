{
  "name": "lab.flipper.net",
  "version": "0.0.1",
  "description": "Web platform for your Flipper",
  "productName": "lab.flipper.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/flipperdevices/lab.flipper.net.git"
  },
  "author": "Maxim Lyubimov",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,sass,css,html,md,json}\" --ignore-path .eslintignore --config .prettierrc.json && yarn lint-fix",
    "dev/web": "quasar dev",
    "dev/electron": "quasar dev -m electron",
    "dev/electron/devtools": "quasar dev -m electron --devtools",
    "build/web": "PRODUCTION=TRUE quasar build -m spa",
    "build/web/dev": "DEV=TRUE quasar build -m spa",
    "build/electron": "PRODUCTION=TRUE quasar build -m electron",
    "build/electron/dev": "DEV=TRUE quasar build -m electron --debug",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.2.1",
    "lodash": "^4.17.21",
    "nanoevents": "^9.0.0",
    "pako": "^2.1.0",
    "pinia": "^2.0.11",
    "protobufjs": "^7.3.2",
    "quasar": "^2.16.0",
    "semver": "^7.6.3",
    "simple-async-sleep": "^1.0.3",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "xterm-addon-serialize": "^0.11.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@quasar/app-vite": "^1.9.0",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.0-beta.10",
    "@types/lodash": "^4.17.6",
    "@types/node": "^12.20.21",
    "@types/pako": "^2.0.3",
    "@types/w3c-web-serial": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "electron": "^31.1.0",
    "electron-builder": "^24.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "main": "index.js",
  "license": "MIT"
}
