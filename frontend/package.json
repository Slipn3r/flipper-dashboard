    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "protobufjs-cli": "^1.1.2",
    "typescript": "^4.5.4",
