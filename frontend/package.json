    "@lezer/highlight": "^1.2.1",
    "@quasar/extras": "^1.16.4",
    "@types/loglevel": "^1.6.3",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.2.1",
    "base64-arraybuffer": "^1.0.2",
    "codemirror": "^6.0.1",
    "d3": "^7.9.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.1",
    "nanoevents": "^9.0.0",
    "pako": "^2.1.0",
    "pinia": "^2.0.11",
    "protobufjs": "^7.3.2",
    "quasar": "^2.16.0",
    "semver": "^7.6.3",
    "simple-async-sleep": "^1.0.3",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "winston": "^3.17.0"
